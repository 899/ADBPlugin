{
  "name": "ADB",
  "version": "0.9.5",
  "description": "ADB Plugin for remote debugging Chrome on Android",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png",
    "48":  "icon_48.png"
  },
  "background": {
    "page":"background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "off_19.png",
      "38": "off_38.png"
    },
    "default_title": "ADB Plugin",
    "default_popup": "popup.html"
  },
  "plugins": [
    { "path": "npADBPlugin.plugin", "public": true },
    { "path": "npADBPlugin.dll", "public": true },
    { "path": "npADBPlugin-x64.so", "public": true },
    { "path": "npADBPlugin-ia32.so", "public": true }
  ]
}
